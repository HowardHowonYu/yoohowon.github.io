{
    "name": "danialnamportfolio",
    "version": "1.0.0",
    "description": "Danial Nam Portfolio For NodeJs",
    "main": "server.js",
    "author": "Danial DaeHyun Nam",
    "license": "MIT",
    "private": true,
    "paths": {
        "src": {
            "base": "./src/",
            "css": "./src/css/",
            "fontello": "./src/fontello/",
            "fonts": "./src/fonts/",
            "json": "./src/json/",
            "js": "./src/js/",
            "html": "./src/html/",
            "scss": "./src/scss/"
        },
        "dist": {
            "base": "./public/",
            "css": "./public/css/",
            "js": "./public/js/",
            "html": "./public/html/",
            "fonts": "./public/fonts/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/css/",
            "fontello": "./build/fonts/fontello/",
            "fonts": "./build/fonts/",
            "js": "./build/js/",
            "html": "./build/html/"
        },
        "favicon": {
            "src": "./src/img/favicon_src.png",
            "dest": "./public/img/site/",
            "path": "/img/site/"
        },
        "img": "./img/",
        "scss": [
            "../_variables"
        ],
        "templates": "./templates/"
    },
    "urls": {
        "live": "https://nystudio107.com/",
        "local": "https://nystudio107.dev/",
        "critical": "https://nystudio107.com/"
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "siteJsName": "site.combined.min.js",
        "scssName": "**/*.scss",
        "cssName": "**/*.css",
        "htmlName": "**/*.html"
    },
    "globs": {
        "distCss": [
            "./build/css/**/*.css",
            "./src/css/**/*.css"
        ],
        "img": [
            "./public/img/"
        ],
        "fonts": [
            "./build/fonts/fontello/font/*.{eot,ttf,woff,woff2}",
            "./src/fonts/*.{eot,ttf,woff,woff2}"
        ],
        "critical": [
            {
                "url": "",
                "template": "index"
            }
        ],
        "download": [
            {
                "url": "https://www.google-analytics.com/analytics.js",
                "dest": "./public/js/"
            }
        ],
        "distJs": [
            "./build/js/site.combined.min.js"
        ],
        "babelJs": [
            "./src/js/**/*.js"
        ],
        "inlineJs": [
            "./node_modules/fg-loadcss/src/loadCSS.js",
            "./node_modules/fg-loadcss/src/cssrelpreload.js",
            "./node_modules/fontfaceobserver/fontfaceobserver.js",
            "./node_modules/loadjs/dist/loadjs.min.js",
            "./node_modules/tiny-cookie/tiny-cookie.min.js"
        ],
        "siteIcon": "./public/img/site/favicon.*"
    },
    "dependencies": {},
    "devDependencies": {
        "babel-plugin-angularjs-annotate": "^0.9.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.16.0",
        "chalk": "^1.1.3",
        "critical": "^1.1.0",
        "fancy-log": "^1.3.2",
        "git-rev-sync": "^1.7.1",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^3.1.0",
        "gulp-babel": "^6.1.2",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.1",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^2.1.2",
        "gulp-download": "^0.0.1",
        "gulp-favicons": "^2.2.6",
        "gulp-filter": "^5.0.1",
        "gulp-fontello": "^0.4.6",
        "gulp-header": "^1.8.7",
        "gulp-htmlmin": "^4.0.0",
        "gulp-if": "^2.0.1",
        "gulp-imagemin": "^3.1.1",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.5.0",
        "gulp-newer": "^1.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-print": "^2.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-rev": "^7.1.0",
        "gulp-sass": "^2.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.2.1",
        "gulp-streamify": "1.0.2",
        "gulp-uglify": "^1.5.4",
        "moment": "^2.22.0",
        "pa11y": "^4.11.0"
    },
    "scripts": {
      "serve" : "http-server ./ -o"
    },
    "bugs": {
        "email": "danialnam89@gmail.com"
    }
}
